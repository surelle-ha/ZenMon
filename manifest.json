{
  "manifest_version": 3,
  "name": "ZenMon",
  "version": "3.0",
  "description": "Zendesk customized tool",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.zendesk.com/agent/*"],
      "js": ["content-script.js"]
    }
  ]
}
